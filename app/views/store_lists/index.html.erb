<% content_for :sidebar do %>
  <%= render 'layouts/main_sidebar' %>
<% end %>
<%= link_to "New List", new_store_list_path %>
<% for list in @store_lists %>
  <div class="store_list_list">
    <strong>Name:</strong> <%= link_to list.name, store_list_path(list) %>
    &nbsp; <%= link_to "edit", edit_store_list_path(list) %>
    </br>
  </div>
<% end %>


