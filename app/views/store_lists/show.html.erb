<% content_for :sidebar do %>
  <%= render 'layouts/main_sidebar' %>
<% end %>

<span class="largefont1"><%= I18n.t(@title) %><%= @store_list.name %></span>
<%= link_to "(edit)", edit_store_list_path(@store_list) %><br/><br/>
  
<div id="right-float-container">
  <%= render 'list_sidebar' %>
  <div>
    <% for store in @store_list.stores %>
      <div class="store">
        <%= render 'stores/store_fields', :store => store %>
      </div>
      <br/>
    <% end %>
  </div>
</div>



